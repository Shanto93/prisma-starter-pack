{
  "name": "prisma-starter-pack",
  "version": "1.0.0",
  "description": "this is a starter pack for prisma with express and typescript",
  "main": "./src/server.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "db:generate": "prisma generate --schema=./prisma/schema",
    "db:migrate": "prisma migrate --schema=./prisma/schema",
    "db:push": "prisma db push --schema=./prisma/schema",
    "db:pull": "prisma db pull --schema=./prisma/schema",
    "db:studio": "prisma studio --schema=./prisma/schema",
    "postinstall": "prisma generate --schema=./prisma/schema",
    "stripe:webhook": "stripe listen --forward-to localhost:5000/webhook"
  },
  "keywords": [],
  "author": "Shanta Islam",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@prisma/client": "^7.2.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "prisma": "^7.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@types/express": "^5.0.6",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "http-status": "^2.1.0",
    "jsonwebtoken": "^9.0.3",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "ts-node-dev": "^2.0.0"
  }
}
